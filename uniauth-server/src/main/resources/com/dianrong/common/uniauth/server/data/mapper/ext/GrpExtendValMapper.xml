<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dianrong.common.uniauth.server.data.mapper.GrpExtendValMapper" >
  <update id="updateSystemDefineGrpAttribute" parameterType="map">
    UPDATE ${tableName} SET ${fieldName} = #{value, jdbcType=VARCHAR} 
    WHERE ${idFiledName} = #{grpId}
  </update>
  
  <update id="updateValue">
        UPDATE grp_extend_val SET value = #{value, jdbcType=VARCHAR} where grp_id
        = #{grpId, jdbcType=INTEGER} and extend_id = #{extendId,
        jdbcType=BIGINT}
    </update>
</mapper>